{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "excerpt": {
      "type": "text"
    },
    "content": {
      "type": "richtext"
    },
    "terms": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::term.term",
      "inversedBy": "posts"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "required": true
    }
  }
}
